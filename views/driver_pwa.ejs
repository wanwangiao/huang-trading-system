        // å‰µå»ºç•¶å‰ä»»å‹™HTML
        function createCurrentTaskHTML(task) {
            const statusMap = {
                'assigned': { text: 'å·²æŒ‡æ´¾', class: 'status-assigned' },
                'picked_up': { text: 'å·²å–è²¨', class: 'status-picked-up' },
                'delivering': { text: 'é…é€ä¸­', class: 'status-delivering' }
            };
            
            const status = statusMap[task.status] || { text: 'æœªçŸ¥', class: '' };
            
            return `
